{
    "shared_item_grid": {
        "type": "itemgrid",
        "h_alignment": "center",
        "v_alignment": "center",
        "item_margin": "1,2",
        "item_size": 64,
        "rows": [
            [
                "red",
                "blue",
                "snorkel",
                "steelskin"
            ],
            [
                "boots",
                "gloves",
                "badge",
                "mask"
            ],
            [
                "anykey",
                "seal",
                "ticket",
                "heart"
            ]
        ]
    },
    "tracker_default": {
        "type": "container",
        "background": "#877373",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "v_alignment": "stretch",
                    "content": [
                        {
                            "type": "group",
                            "header": "Items",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "shared_item_grid"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Pinned Locations",
                            "content": {
                                "type": "recentpins",
                                "style": "wrap",
                                "h_alignment": "stretch",
                                "v_alignment": "stretch",
                                "orientation": "vertical",
                                "compact": true,
                                "width": 150
                            }
                        }
                    ]
                },
                {
                    "type": "map"
                }
            ]
        }
    }
}